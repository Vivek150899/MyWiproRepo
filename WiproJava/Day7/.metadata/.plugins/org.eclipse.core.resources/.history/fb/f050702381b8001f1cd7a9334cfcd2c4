package pack2;

public class Main3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String [] arr= new String[45;
		   String str="Kolkata";
		
		
		arr[0]=String.valueOf("Delhi");
		arr[1]=String.valueOf("Mumbai");
		arr[2]=String.valueOf("Pune");
		arr[3]=String.valueOf("Kolkata");
		arr[4]=str;
		
		for (int s:arr) {
			System.out.println(arr[s]);
		}

	}

}
