package pack1;

public class Employee {
	
	double basicSalary;

	public double getBasicSalary() {
		return basicSalary;
	}

	public void setBasicSalary(double basicSalary) {
		if (basicSalary<0) {
		RuntimeException re=new RuntimeException("Invalid Basic salary : " +basicSalary);
		throw re;
		}
		this.basicSalary = basicSalary;
	}
	

}
