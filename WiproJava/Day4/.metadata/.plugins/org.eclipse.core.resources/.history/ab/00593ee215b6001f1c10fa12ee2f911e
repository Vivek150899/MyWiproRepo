package pack1;

public class Circle{
	public int radius;
	
	
	public Circle(int radius) {
		super();
		this.radius = radius;
	}
	


	public Circle() {
		super();
	}



	@Override
	public String toString() {
		return "Wipro";
	}
	
	public boolean equals(Object obj) {
		Circle temp=(Circle)obj;
		if (this.radius==temp.radius) {
			return true;
		}
		else {
			return true;
		}
	}
    
	

}
